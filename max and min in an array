#include <stdio.h>

int main()
{   int i,min,max,n;
    printf("enter the number:");
    scanf("%d",&n);
    int arr[n];
    for( i=0;i<n;i++)
    {
        printf("enter the number " );
        scanf("%d",&arr[i]);
        
    }
    min=arr[0];
    max=arr[0];
    
    for(i=1;i<n;i++)
    {   if(arr[i]<min)
    {
        min=arr[i];
    }
    if(arr[i]>max)
    {
        max=arr[i];
        
    }
    }
  printf("min:%d",min);
  printf("max:%d",max);
  return 0;
}
