#include <stdio.h>

int main()
{   int i,sec_max,max,n;
    printf("enter the number:");
    scanf("%d",&n);
    int arr[n];
    for( i=0;i<n;i++)
    {
        printf("enter the number " );
        scanf("%d",&arr[i]);
        
    }
    max=arr[0];
    sec_max=arr[0];
    
    for(i=1;i<n;i++)
    {
    if(arr[i]>max)
    {
        max=arr[i];
        
    }
  
    if((arr[i]>sec_max)&&(arr[i]<max))
    {
        sec_max=arr[i];
        
    }
    
    
    
    }
   
    printf("second largest elememt is :%d",sec_max);
  return 0;
}
