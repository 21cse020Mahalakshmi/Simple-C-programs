#include<stdio.h>
#include<string.h>
int main()
{   int n,result;
    printf("enter the length of the string:");
    scanf("%d",&n);
    char str[n],str2[n];
    getchar();
       printf("enter the string:");
    scanf("%s",&str); 
 
    
    printf("the length of the string is %d :",strlen(str));
    printf("enter another string:");
    scanf("%s",&str2);
    printf("\nthe concatenated string is %s",strcat(str,str2));
    result=strcmp(str,str2);
    if(result==0)
    {
        printf("\n two strings are equal");
    }
    else{
        printf("\n these two strings are not equal");
    }
    return 0;
}
