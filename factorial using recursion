#include <stdio.h>
int fact(int n);
int main()
{   int n;
    printf("enter");
    scanf("%d",&n);
    int factorial = fact(n);
    printf("The factorial of %d is %d",n,fact);

    return 0;
}
int fact(int n){
    if (n<0)
    {
        return -1;
    }
    else if(n<=1)
    {
        return 1;
    }
    else
    return n*fact(n-1);
}
